infixl 9  !, ~
infixr 8  **
infixl 7  *, /, %
infixl 6  +, -
infixl 5  <<, >>, <<<, >>>
infix  4  <, <=, >, >=, ==, !=
infixr 3  &, ~&,  ^, ~^, &&
infixr 2  |, ~|
infixr 1  &&
infixr 0  ||

!, ~ :: (a) -> a
**, *, /, %, +, -, <<, >>, <<<, >>>, &, ~&, ^, ~^, |, ~| :: (a, a) -> a
<, <=, >, >=, ==, !=, &&, || :: (a, a) -> Bool
